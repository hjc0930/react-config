{
  "name": "project",
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env dotenv -e .env node ./scripts/start.js",
    "start:dev": "cross-env dotenv -e .env.development node ./scripts/start.js",
    "build": "cross-env dotenv -e .env node ./scripts/build.js",
    "build:test": "cross-env dotenv -e .env.test node ./scripts/build.js",
    "build:pre": "cross-env dotenv -e .env.pre node ./scripts/build.js",
    "build:prod": "cross-env dotenv -e .env.production node ./scripts/build.js",
    "dev": "npm run start:dev",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{less,scss,sass}": [
      "stylelint --fix",
      "prettier --write ."
    ],
    "**/*.{js?(x),ts?(x)}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/node": "^20.1.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "commitlint": "^17.6.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv-cli": "^7.2.1",
    "esbuild-loader": "^3.0.1",
    "eslint": "^8.23.0",
    "eslint-config-alloy": "^5.0.0",
    "eslint-plugin-react": "^7.31.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "ip": "^1.1.8",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "classnames": "^2.3.1",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  }
}
